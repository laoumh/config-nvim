{
    "name": "vscode-style-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": ["markdown", "rmd"],
                "path": "./snippets/markdown.json"
            },
            {
                "language": [
                    "djangohtml", 
                    "html",
                    "htmldjango"
                ],
                "path": "./snippets/html.json"
            },
            {
                "language": ["djangohtml", "htmldjango"],
                "path": "./snippets/frameworks/djangohtml.json"
            }
        ]
    }
}

